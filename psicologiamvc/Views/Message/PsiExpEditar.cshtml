
@{
    ViewBag.Title = "PsiExpEditar";
}

@{

    var usuario = (psicologiamvc.Models.Usuarios)Session["usuarioX"];
    var rol = usuario.Id_Rol;

}

<div class="row">

    <div class="col-lg-4">

    </div>

    <div class="col-lg-4">

        <div class="widget-head-color-box navy-bg p-lg text-center">

            <div class="m-b-md">

                <h2 class="font-bold no-margins">

                    Expediente Actualizado

                </h2>

                <small>Satisfactoriamente</small>

            </div>

            <i class="fa fa-thumbs-up fa-5x"></i>

        </div>

        <div class="ibox-content">

            <center>
                @if (rol == 5 || rol == 10)
                {
                    <button class="btn btn-primary dim btn-large-dim" type="button" onclick="direccion()"><i class="fa fa-check"></i></button>
                }
                else
                {
                    <button class="btn btn-primary dim btn-large-dim" type="button" onclick="direccion2()"><i class="fa fa-check"></i></button>
                }

            </center>

        </div>

    </div>

    <div class="col-lg-4">

    </div>

</div>

@section scripts
{
    <script language="javascript">

        function direccion() {

            location.href = '@Url.Action("IndexEvaluacionSupervisorPsi", "EvaluacionPsi")';

        }

        function direccion2() {

            location.href = '@Url.Action("IndexEvaluacionPsi", "EvaluacionPsi")';

        }

    </script>
}